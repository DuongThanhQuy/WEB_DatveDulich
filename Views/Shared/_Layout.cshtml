<body id="main-homepage">

    <div class="wrapper">
        <!--====== LOADER =====-->
        <div class="loader"></div>
              
        
        <!--============= TOP-BAR ===========-->
        @await Html.PartialAsync("_TopBarPartialView.cshtml")
        <!-- end TOP-BAR -->
        @RenderBody()
        
        <!--======================= FOOTER =======================-->
        @await Html.PartialAsync("_FooterPartialView.cshtml")
        <!-- end footer -->
        
        
        <div id="popup-ad" class="modal fade" role="dialog">
