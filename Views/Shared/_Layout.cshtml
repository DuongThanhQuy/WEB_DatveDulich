                <div class="modal-content">
        
                    <div class="modal-body">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        
                        <div class="row">
                            <div class="col-12 col-md-6">
                                <div class="popup-ad-text">
                                    <h4>Get</h4>
                                    <h2><span>20%</span> off</h2>
                                    <h4>on all flights booking</h4>
                                    <p>Lorem ipsum dolor sit amet, ad duo fugit aeque fabulas, in lucilius prodesset pri.</p>
                                    <a href="~/customerassets/#" class="btn btn-orange">Book Now</a>
                                </div><!-- end popup-ad-text -->
                            </div><!-- end columns -->
                            
                            <div class="col-12 col-md-6">
                                <div class="popup-ad-img">
                                    <img src="~/customerassets/images/about-content-2.png" class="img-fluid" /> 
                                </div><!-- end popup-ad-img -->
                            </div><!-- end columns -->
                        </div><!-- end row -->
                        
                    </div><!-- end modal-bpdy -->
                </div><!-- end modal-content -->
            </div><!-- end modal-dialog -->
        </div><!-- end popup-ad -->
    </div>
                        
                        
    <!-- Page Scripts Starts -->
