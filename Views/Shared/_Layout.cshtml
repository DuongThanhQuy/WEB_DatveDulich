    <script src="~/customerassets/js/bootstrap-datepicker.js"></script>
    <script src="~/customerassets/js/owl.carousel.min.js"></script>
    <script src="~/customerassets/js/custom-navigation.js"></script>
    <script src="~/customerassets/js/custom-flex.js"></script>
    <script src="~/customerassets/js/custom-owl.js"></script>
    <script src="~/customerassets/js/custom-date-picker.js"></script>
    <script src="~/customerassets/js/custom-video.js"></script>
    <script src="~/customerassets/js/popup-ad.js"></script>
    <!-- Page Scripts Ends -->
    @await Component.InvokeAsync("Notyf")
    @await RenderSectionAsync("Scripts", required: false)

